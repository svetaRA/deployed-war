pipeline {

    agent any
    
    stages {
    
       stage('install stage') {
              steps {
              echo "  bat "mvn install"'
        }
    }
        stage('Compile stage') {
            steps {
             echo   "bat "mvn clean compile" "
        }
    }

         stage('testing stage') {
             steps {
             echo "   bat "mvn test""
        }
    }

        /*  stage('deployment stage') {
              steps {
                bat "mvn deploy"
        }
    }*/

  }
 } 
